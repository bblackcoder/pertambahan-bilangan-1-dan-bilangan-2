#include<stdio.h>
#include<conio.h>
//dr.hades007
//https://github.com/drhades007
main() {

	int a,b,hasil;
	
	printf("masukkan bilangan \n");
	printf("bil 1	:");
	scanf("%d",&a);//menginputkan suatu alamat yang dituju
	printf("bil 2	:");
	scanf("%d",&b);//menginputkan suatu alamat yang dituju
	hasil=a+b;//menambahkan bilangan 1 dan bilangan ke dua dengan operator aritmatika pertambahan
	printf("hasil :",hasil);//mengoutputkan hasil dari perjumlahan tersebut
}
